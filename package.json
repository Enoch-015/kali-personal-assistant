{
  "name": "kali-personal-assistant",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter web dev",
    "dev:ai": "pnpm --filter ai dev",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:ai\"",
    "build": "pnpm --filter web build",
    "build:ai": "pnpm --filter ai build",
    "lint": "pnpm --filter web lint",
    "lint:fix": "pnpm --filter web lint:fix"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "lucide-vue-next": "^0.544.0"
  }
}
